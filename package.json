  },
  "dependencies": {
    "@sentry/browser": "^5.17.0",
    "browserify": "latest",
    "sass": "latest"
  },
  "devDependencies": {
    "cypress": "^4.8.0",
    "prettier": "latest",
    "watchify": "latest"
  }
}
